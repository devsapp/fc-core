{
  "name": "@serverless-devs/fc-core",
  "version": "0.0.5",
  "description": "This is a component demo for Serverless Devs Tool ",
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "autoInstall": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "preln": "rm -rf ~/.s/components/devsapp.cn/devsapp/fc-core/dist",
    "ln": "ln -s $(pwd)/dist ~/.s/components/devsapp.cn/devsapp/fc-core/dist",
    "start": "npm run watch",
    "prestart": "npm run prebuild",
    "watch": "tsc -w",
    "prebuild": "rm -rf node_modules && rm -rf package-lock.json && npm i && rimraf dist",
    "esbuild": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=dist/index.js",
    "build": "npm run esbuild"
  },
  "dependencies": {
    "@alicloud/fc2": "latest",
    "@serverless-devs/core": "latest",
    "ali-oss": "^6.17.1",
    "command-exists": "^1.2.9",
    "draftlog": "^1.0.13",
    "form-data": "^4.0.0",
    "git-ignore-parser": "0.0.2",
    "ignore": "^5.1.8",
    "lodash": "^4.17.21",
    "minimatch": "^5.0.0",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "dockerode": "^3.3.1",
    "esbuild": "^0.13.8",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  }
}
